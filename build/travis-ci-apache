<VirtualHost *:80>
  DocumentRoot %TRAVIS_BUILD_DIR%

  <Directory "%TRAVIS_BUILD_DIR%/">
    Options FollowSymLinks
    AllowOverride All
    Require all granted
  </Directory>

  <FilesMatch ".php$"> 
    SetHandler "proxy:unix:/var/run/php/php7.3-fpm.sock|fcgi://localhost/"          
  </FilesMatch>
</VirtualHost>
